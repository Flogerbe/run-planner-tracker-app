<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Run Plan Index</title>
</head>
<body>
    
    <h1>Run Plan Index</h1>
    <table>
        <thead>
            <th>Title</th>
            <th>Distance</th>
            <th>Duration (weeks)</th>
        </thead>
        <tbody>
            <% plans.forEach(function(plan) { %>
                <tr>
                    <td><%= plan.title %></td>
                    <td><%= plan.distance %></td>
                    <td><%= plan.duration %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>

</body>
</html>